{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"extends": "airbnb",
	"rules": {
		"semi": [
			0
        ],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "import/extensions": ["js", "jsx"]
	}
}
